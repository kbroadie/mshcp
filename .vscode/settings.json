{
    "asciidoc.preview.useEditorStyle": false,
    "asciidoc.preview.style": "${workspaceRoot}/themes/styles.css",
    "asciidoc.pdf_generator": "asciidoctor-pdf",
    "asciidoc.pdf_themePath": "${workspaceRoot}/themes/mshcp-theme.yml",
    "asciidoc.preview.refreshInterval": 500,
    "asciidoc.preview.attributes": {
        "experimental": true,
        "icons": "font"
    },
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "cSpell.words": [
        "adoc",
        "Coachella",
        "imagesdir",
        "MSHCP"
    ],
    "cSpell.enabled": true
}